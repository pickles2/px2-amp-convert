{
    "name": "tomk79/px2-amp-convert",
    "description": "Pickles 2 コンテンツを AMP に変換します。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "scripts":{
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ]
    } ,
    "repositories": [
    ],
    "require": {
        "php": ">=5.4.0",
        "tomk79/amp-convert": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*" ,
        "tomk79/filesystem": "1.*",
        "pickles2/broccoli-module-plain-html-elements": "dev-develop",
        "pickles2/px2-px2dthelper": "dev-develop",
        "pickles2/px2-multitheme": "dev-develop",
        "pickles2/px-fw-2.x": "dev-develop",
        "phpdocumentor/phpdocumentor": "^2.9"
    },
    "autoload": {
        "files": [
            "php/main.php",
            "php/utils.php"
        ]
    },
    "extra": {
        "px2package": {
            "name": "tomk79/px2-amp-convert",
            "type": "processor"
        }
    }
}
